<!DOCTYPE html>
<html>
<head>
  <title>ArsBlog</title>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>

<body>

<%= render partial: 'layouts/header' %>

<div class="container">

  <div class="row">
    <div class="col-md-7">
      <%= yield %>
    </div>
    <div class="col-md-5">
      <%= render partial: 'layouts/right_trail' if @right_trail_content %>

    </div>
  </div>


  <div class="row">
    <div class="col-md-12">
      <hr>
    </div>
  </div>

</div>

</body>